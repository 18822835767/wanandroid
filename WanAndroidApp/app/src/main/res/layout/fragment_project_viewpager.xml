<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.v4.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/projectFragment_SwipeRefreshLayout_refresh">


        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/projectFragment_listView_showProjectItem">
        </ListView>

    </android.support.v4.widget.SwipeRefreshLayout>

    <com.example.sorena.wanandroidapp.widget.FloatingButtonLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:id="@+id/projectFragment_fb_toTop">

    </com.example.sorena.wanandroidapp.widget.FloatingButtonLayout>


</RelativeLayout>
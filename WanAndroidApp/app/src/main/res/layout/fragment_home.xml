<?xml version="1.0" encoding="utf-8"?>
<com.example.sorena.wanandroidapp.widget.SwipeRefreshLayoutIncludeVP
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/home_swipeRefreshLayout_refreshData"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/home_listView_showArticle"
            android:dividerHeight="10dp"
            android:divider="#00000000"
            android:layout_margin="10dp"
            >
        </ListView>

        <com.example.sorena.wanandroidapp.widget.RefreshLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/mainActivity_refresh">
        </com.example.sorena.wanandroidapp.widget.RefreshLayout>

        <com.example.sorena.wanandroidapp.widget.FloatingButtonLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:id="@+id/homeFragment_fb_toTop">

        </com.example.sorena.wanandroidapp.widget.FloatingButtonLayout>

    </RelativeLayout>


</com.example.sorena.wanandroidapp.widget.SwipeRefreshLayoutIncludeVP>